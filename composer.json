{
  "name": "codeinc/redirect-middleware",
  "version": "1.0.0",
  "description": "A PSR-15 middleware to handle redirections",
  "homepage": "https://github.com/CodeIncHQ/GoogleOAuth2Middleware",
  "type": "library",
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "psr/http-message": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "psr/http-server-handler": "^1.0",
    "codeinc/psr7-responses": "^1",
    "firebase/php-jwt": "^5.0",
    "codeinc/middleware-test-kit": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7",
    "codeinc/psr7-response-sender": "^1"
  },
  "autoload": {
    "psr-4": {"CodeInc\\RedirectMiddleware\\": "src/"}
  },
  "autoload-dev": {
    "psr-4": { "CodeInc\\RedirectMiddleware\\Tests\\": "tests/" }
  },
  "authors": [
    {
      "name": "Joan Fabr√©gat",
      "email": "joan@codeinc.fr",
      "homepage": "https://www.codeinc.fr",
      "role": "developer"
    }
  ],
  "config": {
    "preferred-install": {
      "*": "dist"
    }
  }
}
